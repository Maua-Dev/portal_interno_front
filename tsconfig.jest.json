{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "jsx": "react",
        "esModuleInterop": true,
        "strict": true,
        "moduleResolution": "node",
        "baseUrl": ".",
        "paths": {
        "@/components/": ["components/"],
        "@/lib/": ["lib/"],
        "@/pages/": ["pages/"],
        "@/public/": ["public/"],
        "@/styles/": ["styles/"],
        "@/types/": ["types/"],
        "@/utils/": ["utils/"]
        },
        "skipLibCheck": true
    }
}