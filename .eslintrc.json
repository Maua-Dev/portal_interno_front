{
  "extends": ["next/core-web-vitals", "@rocketseat/eslint-config/react"],
  "rules": {
    "semi": "off",
    "quotes": ["warn", "single"],
    "prettier/prettier": "warn",
    "comma-dangle": "off",
    "no-new": "off",
    "no-unused-vars": "warn",
    "no-unused-expressions": "warn",
    "no-unused-labels": "warn",
    "accessor-pairs": "off"
  }
}